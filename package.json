{
  "name": "node-js-TS",
  "version": "1.0.0",
  "description": "node-js-TS",
  "scripts": {
    "start": "tsc && node ./dist/server.js | ./node_modules/.bin/bunyan",
    "server": "tsc && nodemon ./dist/server.js | ./node_modules/.bin/bunyan",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"tsc -w\" \"npm run server\" \"npm run client\"",
    "start-server": "./node_modules/pm2/bin/pm2 start ./dist/server.js",
    "stop-server": "./node_modules/pm2/bin/pm2 stop ./dist/server.js",
    "logs": "./node_modules/pm2/bin/pm2 logs",
    "test": "NODE_ENV=test ./node_modules/pm2/bin/pm2 start ./dist/server.js --update-env && NODE_ENV=test mocha test --exit --timeout 50000 && NODE_ENV=test ./node_modules/pm2/bin/pm2 stop ./dist/server.js",
    "test-coverage": "nyc npm run test",
    "lint": "npx eslint .",
    "prettier": "npx prettier -c .",
    "db:migrate": "rm -rf ./dist && tsc && npx sequelize db:migrate",
    "db:migrate:create": "npx sequelize migration:generate --name",
    "db:migrate:undo": "tsc && npx sequelize db:migrate:undo",
    "db:migrate:status": "npx sequelize db:migrate:status"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auth0": "^2.36.1",
    "aws-amplify": "^4.2.2",
    "aws-sdk": "^2.1136.0",
    "axios": "^0.22.0",
    "body-parser": "^1.20.0",
    "bunyan": "^1.8.12",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-bunyan-logger": "^1.3.3",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "mysql2": "^2.1.0",
    "newrelic": "^9.0.1",
    "node-schedule": "^2.1.0",
    "otp-generator": "^2.0.1",
    "pm2": "^5.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.19.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/convict": "^6.1.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-bunyan-logger": "^1.3.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.32",
    "@types/sequelize": "^4.28.13",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.3",
    "nyc": "^15.1.0",
    "prettier": "2.4.1",
    "pretty-quick": "^2.0.1",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": "14.17.3",
    "npm": "6.14.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
